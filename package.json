{
  "name": "@egoist/youtube-transcript-plus",
  "version": "1.1.0",
  "description": "Fetch transcript from a YouTube video",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "packageManager": "bun@1.0.0",
  "scripts": {
    "build": "tsdown",
    "format": "prettier --write 'src/**/*.ts'",
    "test": "vitest --no-watch",
    "test:watch": "vitest",
    "prepublishOnly": "bun run build"
  },
  "author": "ericmmartin",
  "keywords": [
    "youtube",
    "transcript"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "https-proxy-agent": "^7.0.6",
    "prettier": "^3.6.2",
    "tsdown": "^0.7.2",
    "tslib": "^2.8.1",
    "typescript": "^5.9.2",
    "vitest": "^2.0.5"
  },
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericmmartin/youtube-transcript-plus.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/ericmmartin/youtube-transcript-plus",
  "engines": {
    "node": ">=18.0.0"
  }
}
